<com:TContent ID="body">

<h1>TActiveFileUpload Sample</h1>

<p>
Please choose a file to upload:
</p>

<com:TActiveFileUpload OnFileUpload="fileUploaded" />

<br/>

<com:TActiveLabel ID="Result" />

<h1>TActiveFileUpload Sample (HTML5 multiple file upload)</h1>

<p>
Please choose one or more files to upload (keep CTRL pressed to select additional files):
</p>

<com:TActiveFileUpload OnFileUpload="fileUploadedMulti" Multiple="true"/>

<br/>

<com:TActiveLabel ID="ResultMulti" />

</com:TContent>
